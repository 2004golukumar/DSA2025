  int floorSqrt(int n) {
        // code here
        int    low=0,high=n,ans;
        if(n==1) return 1;
        while(low<=high){
            int mid=low+(high-low)/2;
            if(mid*mid==n) return mid;
            else if(mid*mid>n){
                high=mid-1;
            }
            else{
                ans=mid;
                low=mid+1;
            }
        }
        return ans;
    }
};
